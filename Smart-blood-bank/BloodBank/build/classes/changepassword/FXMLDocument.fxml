<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="570.0" prefWidth="771.0" stylesheets="@changepassword.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="changepassword.FXMLDocumentController">
   <children>
      <Label fx:id="changePassword" layoutX="213.0" layoutY="70.0" text="Changing Password" />
      <VBox layoutX="192.0" layoutY="155.0" prefHeight="240.0" prefWidth="392.0">
         <children>
            <JFXPasswordField fx:id="actualPassword" focusColor="#d32f2f" labelFloat="true" prefHeight="47.0" prefWidth="392.0" promptText="Your actual password">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
               <font>
                  <Font size="16.0" />
               </font>
            </JFXPasswordField>
            <JFXPasswordField fx:id="newPassword" focusColor="#d32f2f" labelFloat="true" prefHeight="47.0" prefWidth="392.0" promptText="Your new password">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
               <font>
                  <Font size="16.0" />
               </font>
            </JFXPasswordField>
            <JFXPasswordField fx:id="newPassword2" focusColor="#d32f2f" labelFloat="true" prefHeight="47.0" prefWidth="392.0" promptText="Confirm your new password">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
               <font>
                  <Font size="16.0" />
               </font>
            </JFXPasswordField>
         </children>
      </VBox>
      <JFXButton layoutX="258.0" layoutY="424.0" onAction="#change" prefHeight="44.0" prefWidth="114.0" text="Change" />
      <JFXButton layoutX="393.0" layoutY="424.0" onAction="#back" prefHeight="44.0" prefWidth="114.0" text="Cancel" />
   </children>
</AnchorPane>
